<div class="page-header">
    <h1>Weather App</h1>
</div>
<div class="row extraMargin">
    <div class="col-md-12">
        <button class="weatherButtons btn btn-default" data-location="london">Get weather in London</button>
        <button class="weatherButtons btn btn-default" data-location="new-york">Get weather in New York</button>
        <button class="weatherButtons btn btn-default" data-location="tokyo">Get weather in Tokyo</button>
    </div>
</div>
<div class="row">
    <div class="col-md-4 col-md-offset-4  main_info">
        <div class="row">
            <div class="col-md-12 divide">
                <h2 id="city_output" class="lead">Choose a city</h2>
            </div>
            <div class="col-md-6">
                <p id="weather_output" class="lead">Outputs temperature</p>
            </div>
            <div class="col-md-6">
                <p id="time_output" class="lead">Outputs time</p>
            </div>
        </div>
    </div>
</div>